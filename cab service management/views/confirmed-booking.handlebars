      <div class="container">
        <center><h1>My rides</h1></center>
    <table class="table table-striped mt-4">
        <tr>
            <th>Pick up</th>
            <th>Destination</th>
            <th>Time</th>
            <th>Date</th>
            <th>Capacity</th>
            <th>Cost</th>
            <th>First name</th>
            <th>Last name</th>
            <th>Email</th>
            <th>Phone</th>
            <td>Booking status</td>
            <td>Payment status</td>
            
            <th>Action</th>
        </tr>
        
        
      
      
 
   {{#each data}}
            
            <td>{{this.pick_up}}</td>
            <td>{{this.destination}}</td>
            <td>{{this.time}}</td>
            <td>{{this.date}}</td>
            <td>{{this.capacity}}</td>
            <td>{{this.cost}}</td>
            <td>{{this.first_name}}</td>
            <td>{{this.last_name}}</td>
            <td>{{this.email}}</td>
            <td>{{this.phone}}</td>
            <td>{{this.booking_status}}</td>
            <td>{{this.payment_status}}</td>
            
            <td>

                

                <button style="background-color:rgb(56, 53, 53);" onClick=location.href="/api/booking/delete/{{this.booking_id}}" class="btn btn-primary">Cancel</button>
            </td>
        </tr>
   {{/each}}
    </table>
   
